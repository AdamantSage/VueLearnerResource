<h1>Dashboard</h1>
<p>Welcome to the dashboard!</p>

<form action="/dashboard" method="GET">
<h2> Search Videos or documents</h2>
<label>Name</label>
<input type="text" name="name" value ="<%= searchOptions.name %>">
<button type="submit">Search</button>
</form>

<br>
<!--looping through all resources here-->
<%resources.forEach(resources => {%>
    <a href="/dashboard/<%= resources.id %>">
<div><%= resources.title %></div>
<a href="/dashboard/<%= resources.id %>"></a><div><%= resources.link %></div></a>
<!-- add descrption here-->
<% })%>
